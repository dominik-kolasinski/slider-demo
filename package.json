{
  "name": "slider-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bem-react/classname": "^1.5.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@types/node": "12.12.7",
    "@types/node-sass": "^4.11.0",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setup/test-shim.js",
      "<rootDir>/tests/setup/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/tests/setup/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ],
    "collectCoverage": false
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^24.0.22",
    "@types/react-test-renderer": "^16.9.1",
    "@types/redux-mock-store": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.11.0",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^24.1.0"
  }
}
